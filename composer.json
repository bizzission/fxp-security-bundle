{
    "name": "fxp/security-bundle",
    "type": "symfony-bundle",
    "description": "A Extended Role-Based Access Control with sharing rules",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/fxpio/fxp-security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "fxp/security": "dev-master as 2.0",
        "symfony/framework-bundle": "^5.0.0",
        "symfony/security-bundle": "^5.0.0"
    },
    "require-dev": {
        "symfony/form": "^5.0.0",
        "symfony/translation": "^5.0.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-security.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-config.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-doctrine-extra.git"
        }
    ]
}
